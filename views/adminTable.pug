include header

div(class="container")
    h1 ADMIN PANEL/CALLBACKS_PAGE
    div(class='admin-btns')
        a(href="/admin" class='btn btn-primary') Come_back_to_reality 
        a(class="btn btn-dark"  href="/btn-reset" type="reset") RESET
    h3 ИНФОРМАЦИЯ О ЗАЯВКЕ    
    table(class="table table-striped")
        thead
            tr
                th(scope="col") ID
                th(scope="col") NAME
                th(scope="col") NUMBER
                th(scope="col") DATE
        tbody
            each val, index in callbacks    
                tr 
                    th= val["id"]
                    td= val["username"]
                    td
                        a(href="tel:"+val["number"])= val["number"]
                    td= val["unix_timestamp"]              
        
        
    
